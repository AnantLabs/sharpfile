<Settings>
  <ParentType>Dual</ParentType>
  <Width>500</Width>
  <Height>500</Height>
  <SplitterPercentage>50</SplitterPercentage>
  <DirectoriesSortedFirst>true</DirectoriesSortedFirst>
  <Icons>
    <ShowOverlay>true</ShowOverlay>
    <Extensions>
      <Extension>.exe</Extension>
      <Extension>.lnk</Extension>
      <Extension>.dll</Extension>
      <Extension>.ps</Extension>
      <Extension>.scr</Extension>
      <Extension>.ico</Extension>
      <Extension>.icn</Extension>
    </Extensions>
  </Icons>
  <ResourceRetrievers>
    <ResourceRetriever Name="DriveRetriever" ChildResourceRetriever="FileRetriever">
      <FullyQualifiedType Assembly="SharpFile" Type="SharpFile.IO.Retrievers.DriveRetriever" />
    </ResourceRetriever>
    <ResourceRetriever Name="NetworkDriveRetriever" ChildResourceRetriever="FileRetriever">
      <FullyQualifiedType Assembly="SharpFile" Type="SharpFile.IO.Retrievers.NetworkDriveRetriever" />
    </ResourceRetriever>
  </ResourceRetrievers>
  <ChildResourceRetrievers>
    <ChildResourceRetriever Name="FileRetriever">
      <ColumnInfos>
        <ColumnInfo Text="Filename" Property="DisplayName" PrimaryColumn="true">
          <ComparerType Assembly="SharpFile" Type="Common.General.StringLogicalComparer" />
        </ColumnInfo>
        <ColumnInfo Text="Size" Property="Size" PrimaryColumn="false">
          <ComparerType Assembly="SharpFile" Type="Common.General.StringLogicalComparer" />
          <MethodDelegateType Method="GetHumanReadableSize">
            <FullyQualifiedType Assembly="SharpFile" Type="Common.General" />
          </MethodDelegateType>
        </ColumnInfo>
        <ColumnInfo Text="Date" Property="LastWriteTime" PrimaryColumn="false">
          <ComparerType Assembly="SharpFile" Type="Common.General.StringLogicalComparer" />
          <MethodDelegateType Method="GetDateTimeShortDateString">
            <FullyQualifiedType Assembly="SharpFile" Type="Common.General" />
          </MethodDelegateType>
        </ColumnInfo>
        <ColumnInfo Text="Time" Property="LastWriteTime" PrimaryColumn="false">
          <ComparerType Assembly="SharpFile" Type="Common.General.StringLogicalComparer" />
          <MethodDelegateType Method="GetDateTimeShortTimeString">
            <FullyQualifiedType Assembly="SharpFile" Type="Common.General" />
          </MethodDelegateType>
        </ColumnInfo>
      </ColumnInfos>
      <FullyQualifiedType Assembly="SharpFile" Type="SharpFile.IO.Retrievers.FileRetriever" />
    </ChildResourceRetriever>
  </ChildResourceRetrievers>
</Settings>